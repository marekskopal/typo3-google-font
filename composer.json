{
    "name": "marekskopal/typo3-google-font",
    "type": "typo3-cms-extension",
    "description": "Includes Google Font in your TYPO3 website.",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.2",
        "typo3/cms-core": "^13.4 || ^14.0",
        "typo3/cms-frontend": "^13.4 || ^14.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.0",
        "shipmonk/phpstan-rules": "^4.0",
        "slevomat/coding-standard": "^8.14"
    },
    "autoload": {
        "psr-4": {
            "MarekSkopal\\GoogleFont\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ms_google_font"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
